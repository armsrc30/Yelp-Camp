<% layout('layout/boilerplate') %>
<h1>Delete <%=camp.title%>?</h1>
<form action='/campgrounds/<%=camp.id%>/?_method=DELETE' method="POST">
    <button type="submit" class="btn btn-danger">Yes</button>
</form>
<form action='/campgrounds/<%=camp.id%>' method="GET" class="mt-3">
    <button type="submit" class="btn btn-secondary">No, Go Back</button>
</form>

